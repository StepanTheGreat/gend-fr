rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId} {
    	allow write: if request.auth.uid == userId;
      allow read, write: if
          request.time < timestamp.date(2023, 5, 16);
    }
    
    function isSignedIn() {
    	return request.auth != null;
    }
  }
}